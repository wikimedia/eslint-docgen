{
	"name": "eslint-docgen",
	"version": "0.7.1",
	"description": "Automatically generate ESLint plugin documentation from rule metadata and test cases.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wikimedia/eslint-docgen.git"
	},
	"license": "MIT",
	"keywords": [
		"eslint",
		"documentation"
	],
	"main": "src/index.js",
	"bin": {
		"eslint-docgen-build-readme": "src/build-readme.js"
	},
	"files": [
		"src"
	],
	"scripts": {
		"test": "eslint . --cache && nyc mocha tests/",
		"report": "nyc report --reporter=html"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"ejs": "^4.0.1",
		"eslint": ">=8.0.0",
		"import-fresh": "^3.3.1",
		"jsonschema": "^1.5.0",
		"merge-options": "^3.0.4",
		"mkdirp": "^3.0.1",
		"pkg-dir": "^5.0.0",
		"pluralize": "^8.0.0",
		"simple-mock": "^0.8.0",
		"upath": "^2.0.1"
	},
	"devDependencies": {
		"eslint-config-wikimedia": "^0.32.3",
		"mocha": "^11.7.5",
		"nyc": "^17.1.0"
	}
}
