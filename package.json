{
	"name": "eslint-docgen",
	"version": "0.6.2",
	"description": "Automatically generate ESLint plugin documentation from rule metadata and test cases.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wikimedia/eslint-docgen.git"
	},
	"license": "MIT",
	"keywords": [
		"eslint",
		"documentation"
	],
	"main": "src/index.js",
	"files": [
		"src"
	],
	"scripts": {
		"test": "eslint . --cache && nyc mocha tests/",
		"report": "nyc report --reporter=html"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"ejs": "^3.1.6",
		"eslint": ">=8.0.0",
		"import-fresh": "^3.3.0",
		"jsonschema": "^1.4.0",
		"merge-options": "^3.0.4",
		"mkdirp": "^1.0.4",
		"pkg-dir": "^5.0.0",
		"pluralize": "^8.0.0",
		"simple-mock": "^0.8.0",
		"upath": "^2.0.1"
	},
	"devDependencies": {
		"eslint-config-wikimedia": "^0.22.1",
		"eslint-plugin-node": "^11.1.0",
		"mocha": "^9.2.0",
		"nyc": "^15.1.0"
	}
}
